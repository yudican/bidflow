{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "barryvdh/laravel-dompdf": "^1.0",
        "diglactic/laravel-breadcrumbs": "^7.2",
        "doctrine/dbal": "^3.3",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "irazasyed/telegram-bot-sdk": "^3.13",
        "laravel/framework": "^9.19",
        "laravel/jetstream": "^2.3",
        "laravel/passport": "^10.3",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "^3.5",
        "livewire/livewire": "^2.10",
        "maatwebsite/excel": "^3.1",
        "mediconesystems/livewire-datatables": "^0.9.5",
        "midtrans/midtrans-php": "^2.5",
        "milon/barcode": "^9.0",
        "picqer/php-barcode-generator": "^2.4",
        "predis/predis": "^2.0",
        "psr/simple-cache": "^2.0",
        "pusher/pusher-php-server": "^7.2",
        "rappasoft/laravel-livewire-tables": "^2.7",
        "sentry/sentry-laravel": "^2.12",
        "simplesoftwareio/simple-qrcode": "^4.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "*",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.3.3",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "exclude-from-classmap": [
            "vendor/livewire/livewire/src/Controllers/FilePreviewHandler.php",
            "vendor/livewire/livewire/src/Controllers/FileUploadHandler.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php",
            "app/overrides/livewire/FilePreviewHandler.php",
            "app/overrides/livewire/FileUploadHandler.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "platform-check": false
}
